{
    "compilerOptions": {
      "target": "ESNext",
      "module": "ESNext",
      "moduleResolution": "Node",
      "jsx": "preserve",
      "strict": true,
      "baseUrl": ".",
      "paths": {
        "@/*": ["./*"]
      },
      "allowJs": true,
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "skipLibCheck": true
    },
    "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
    "exclude": ["node_modules"]
  }
  